{
  "name": "stroll-web-client",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "khirayama",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf dist && npm run compile:scripts && (npm run compile:scripts:watch & npm run bundle:scripts:watch & nodemon dist/server.js)",
    "compile:scripts": "babel src --out-dir dist --ignore src/client.js",
    "compile:scripts:watch": "babel src --out-dir dist -w --source-maps inline --ignore src/client.js",
    "bundle:scripts": "webpack -p",
    "bundle:scripts:watch": "webpack -w",
    "test": "ava",
    "test:watch": "ava -w"
  },
  "devDependencies": {
    "ava": "^0.19.1"
  },
  "dependencies": {
    "@khirayama/circuit": "^1.1.1",
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.2",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "webpack": "^2.4.1"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-core/register"
    ]
  }
}
